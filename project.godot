config_version=5

[application]

config/name="2D Test"
run/main_scene="res://startup.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.svg"

[autoload]

Globals="*res://global_variables.gd"
Helpers="*res://helper_functions.gd"
Network="*res://network_websocket.gd"
Spawner="*res://spawner.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/integer_division=0

[display]

window/size/always_on_top=true

[editor_plugins]

enabled=PackedStringArray("res://addons/Mirror/plugin.cfg", "res://addons/jwt/plugin.cfg")

[filesystem]

import/blender/enabled=false

